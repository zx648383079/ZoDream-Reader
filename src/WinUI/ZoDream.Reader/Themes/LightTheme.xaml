<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:ZoDream.Reader.Themes">
    <SolidColorBrush x:Key="WindowHostBrush" Color="#F0F3F9" />
    <SolidColorBrush x:Key="WindowCaptionBackground" Color="#F0F3F9" />
    <SolidColorBrush x:Key="WindowCaptionBackgroundDisabled" Color="#F0F3F9" />
    <SolidColorBrush x:Key="TranspantBackground"
        Opacity="0.001"
        Color="White" />
    <AcrylicBrush
        x:Key="AppMaskAcrylicBrush"
        FallbackColor="White"
        TintColor="White"
        TintOpacity="0.5" />
</ResourceDictionary>
